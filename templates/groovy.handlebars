    {{model.name}}(WebDriver driver) {
        super(driver)
    }

    //Click elements
{{#each definitions}}
    {{../model.name}} click{{attribute.idforgroovy}}(){
        clickElement({{attribute.idforgroovy}})
    }

{{/each}}

    //SetText on elements
{{#each definitions}}
    {{../model.name}} set{{attribute.idforgroovy}}Text(String s){
        sendKeys({{attribute.idforgroovy}}, s)
    }

{{/each}}


    //isDisplay on elements
{{#each definitions}}
    boolean is{{attribute.idforgroovy}}Displayed(){
        isDisplayed({{attribute.idforgroovy}})
    }

{{/each}}

    //isNotDisplay on elements
{{#each definitions}}
